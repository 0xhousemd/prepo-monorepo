{
  "name": "prepo-react-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "generate-types": "typechain --target=ethers-v5 'abi/*.json' --out-dir generated/typechain",
    "postinstall": "yarn generate-types && yarn mst-gql:generate",
    "dev": "npx next",
    "build": "next build && next-sitemap",
    "test": "jest -w 1",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "mst-gql:generate": "./internals/mst-gql-generate.bash"
  },
  "dependencies": {
    "@prepo-io/constants": "0.10.1",
    "@prepo-io/stores": "0.15.2",
    "@prepo-io/utils": "0.7.2",
    "@sentry/nextjs": "6.17.6",
    "antd": "4.19.5",
    "bnc-onboard": "1.36.0",
    "clone-deep": "4.0.1",
    "configcat-js": "5.6.0",
    "ethereum-multicall": "2.10.0",
    "ethers": "5.4.7",
    "graphql": "15.7.2",
    "graphql-tag": "2.12.6",
    "mobx": "6.3.8",
    "mobx-react-lite": "3.2.1",
    "mobx-state-tree": "5.1.0",
    "mst-gql": "0.14.1",
    "next": "12.0.7",
    "next-sitemap": "1.6.203",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-jazzicon": "0.1.3",
